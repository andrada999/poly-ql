<Window x:Class="QSForm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="130" />
            <RowDefinition Height="130" />
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <TextBox Name="tbOutput" Grid.Row="1" />
        <TextBox Name="tbInput" Grid.Row="0" AcceptsReturn="True" AcceptsTab="True" />
        <Button Grid.Row="2" Content="Parse" Click="Button_Click"/>
    </Grid>
</Window>
